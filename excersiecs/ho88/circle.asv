clc;
close all;
clear;

r = 50;
H = 1000;
W = 1000;
x = 300;
y = 500;
cr = circle1(x, y, r);
imshow(cr);

function [f] = circle1(W, H, x, y, r)

for m=1 : 1 : H
    for n=1 : 1 : W
        
        if sqrt(power(m-y)) >= r
           f(m,n)=0;
        else
            f(m,n)=1;
        end
       
    end
end

end